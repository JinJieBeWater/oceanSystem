{
  "files.associations": {
    "manifest.json": "jsonc",
    "pages.json": "jsonc",
    "*.md": "markdown",
    "*.css": "tailwindcss",
    "qapplication": "cpp",
    "qdebug": "cpp"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.patterns": {
    "*.cpp": "$(capture).hpp, $(capture).h, $(capture).hxx, $(capture).hh, $(capture).ui",
  },
}
